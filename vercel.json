{
  "version": 2,
  "buildCommand": "npm ci --prefix webui && npm run build --prefix webui",
  "outputDirectory": "static/admin",
  "rewrites": [
    {
      "source": "/admin/login",
      "destination": "/api/index"
    },
    {
      "source": "/admin/verify",
      "destination": "/api/index"
    },
    {
      "source": "/admin/config",
      "destination": "/api/index"
    },
    {
      "source": "/admin/keys(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin/accounts(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin/queue/status",
      "destination": "/api/index"
    },
    {
      "source": "/admin/import",
      "destination": "/api/index"
    },
    {
      "source": "/admin/test",
      "destination": "/api/index"
    },
    {
      "source": "/admin/vercel/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin/export",
      "destination": "/api/index"
    },
    {
      "source": "/admin/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/admin/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}
